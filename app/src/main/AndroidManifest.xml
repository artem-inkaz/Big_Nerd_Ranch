<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.big_nerd_ranch">

    <application
        android:name=".CriminalIntentApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_cherepaha"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Big_Nerd_Ranch">
        <activity android:name=".CheatActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CrimeIntent.FragmentCriminalIntent" />
    </application>

</manifest>